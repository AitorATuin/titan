function booleanops(): int
    return not 1 or 2 and 3 and 4
end

function relational(): int
    return (1 <= 2) == (4 < 5) == (6 ~= 7)
end

function bitwise(): int
    return ~~1 ~ 2 << 3 >> 4 | 5 & 6
end

function concat(): string
    return "a" .. "b" .. "c" -- right assoc
end

function arithmetic(): int
    return - -1 / 2 + 3 * # "a"
end

function pow(): float
    return -1 ^ -2 ^ 3 * 4 -- right assoc, stronger than prefix
end

function concat2(): string
    return x .. y .. z -- right assoc
end

function concat3(): string
    return 1 .. 2
end

function concat4(): string
    return 2.5 .. 1.5
end

function concat5(): string
    return "a" .. 2 -- right assoc
end

function concat6(): string
    return 2 .. "a" -- right assoc
end
